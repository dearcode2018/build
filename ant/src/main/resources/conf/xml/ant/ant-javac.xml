<?xml version="1.0" encoding="UTF-8"?>
<project name="ant" default="compile" basedir=".">

	
	<!-- 编译之前先清理 -->
	<target name="clean">
		<delete dir="build" />
	</target>


	<!-- target item -->
	<target name="compile" depends="clean">
		<javac srcdir="src/main/java" destdir="target/classes">
			
		</javac>
		<description> target description</description>
	</target>

</project>