<?xml version="1.0" encoding="UTF-8"?>
<project name="delete" default="deleteFile" basedir=".">
	<!--  -->
	<!-- 删除文件 -->
	<target name="deleteFile">
		<description>删除文件</description>
		<!-- 删除文件 -->
		<delete>
			<fileset dir="${basedir}/copy">
				<!-- 包含哪些文件 -->
			    <include name="ant-jav*.jar"/>
				<!-- 排除哪些文件 -->
			    <exclude name="ant-jar*.jar"/>
			</fileset>
		</delete>
	</target>
	
	<!-- 删除目录 -->
	<target name="deleteDirectory">
		<description>删除目录</description>
		<delete dir="${basedir}/copy">
		</delete>
	</target>
	
</project>