<?xml version="1.0" encoding="UTF-8"?>
<project name="prjNname" default="">

	<!-- 平台环境变量 -->
    <property environment="env" />
	<!-- 项目根路径 (仅针对当前目录有效) -->
	<property name="project.root" value="${basedir}../../../../" />
	
	<!-- 引入外部属性文件 -->
	<property file="${project.root}/conf/properties/ant.properties"></property>
	

	<!-- 输出指定的环境变量 -->
	<echo message="${env.JAVA_HOME}"></echo>
	<echo message="${env.ANT_HOME}"></echo>
	
	<echo message="${project.root}"></echo>
	
	
	
	
</project>